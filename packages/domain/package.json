{
  "name": "@ye/domain",
  "version": "0.1.0",
  "private": true,
  "description": "Core package for business logic",
  "license": "MIT",
  "author": "Bois",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "primitives": {
      "types": "./dist/primitives/index.d.ts",
      "default": "./dist/primitives/index.js"
    },
    "value-objects": {
      "types": "./dist/value-objects/index.d.ts",
      "default": "./dist/value-objects/index.js"
    },
    "entities": {
      "types": "./dist/entities/index.d.ts",
      "default": "./dist/entities/index.js"
    },
    "aggregates": {
      "types": "./dist/aggregates/index.d.ts",
      "default": "./dist/aggregates/index.js"
    }
  },
  "scripts": {
    "execute": "tsx ./src/execute.ts",
    "build": "tsc -p tsconfig.build.json",
    "clean": "bun run rm -rf dist *.tsbuildinfo .turbo node_modules",
    "dev": "tsc -w -p tsconfig.build.json",
    "typecheck": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check . --write"
  },
  "devDependencies": {
    "@ye/tsconfig": "workspace:^",
    "tsx": "^4.19.2"
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "effect": "^3.9.1"
  }
}

{
  "name": "@ye/i18n",
  "version": "0.1.0",
  "private": true,
  "description": "A package to share Storybook configuration preset for the workspace",
  "license": "MIT",
  "author": "Bois",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "bun run rm -rf dist *.tsbuildinfo .turbo node_modules",
    "dev": "tsc -w -p tsconfig.build.json",
    "typecheck": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "postinstall": "tsx ./bin/generate.ts"
  },
  "dependencies": {
    "@ye/primitives": "workspace:^",
    "next-intl": "^3.26.3"
  },
  "devDependencies": {
    "tsx": "latest",
    "@ye/tsconfig": "workspace:^"
  }
}

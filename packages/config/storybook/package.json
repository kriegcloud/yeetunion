{
  "name": "@ye/storybook",
  "version": "0.1.0",
  "private": true,
  "description": "A package to share Storybook configuration preset for the workspace",
  "license": "MIT",
  "author": "Bois",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./configs/react-vite": {
      "types": "./dist/configs/react-vite.d.ts",
      "default": "./dist/configs/react-vite.js"
    },
    "./manager": {
      "types": "./dist/manager.d.ts",
      "default": "./dist/manager.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "default": "./dist/theme.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "bun run rm -rf dist *.tsbuildinfo .turbo node_modules",
    "dev": "tsc -w -p tsconfig.build.json",
    "typecheck": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check . --write"
  },
  "dependencies": {
    "@ye/assets": "workspace:^",
    "@ye/theme": "workspace:^",
    "@ye/utils": "workspace:^",
    "@radix-ui/colors": "^3.0.0",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/addon-viewport": "^8.4.7",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@storybook/types": "^8.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.4.7"
  },
  "devDependencies": {
    "@ye/tsconfig": "workspace:^",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.1"
  }
}

{
  "name": "@ye/meta",
  "version": "0.1.0",
  "private": true,
  "description": "Common utilities for TheBois",
  "license": "MIT",
  "author": "Bois",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./app-routes": {
      "types": "./dist/app-routes.d.ts",

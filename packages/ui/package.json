{
  "name": "@ye/ui",
  "version": "0.1.0",
  "private": true,
  "description": "Common utilities for TheBois",
  "license": "MIT",
  "author": "Bois",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#*": "./dist/*"
  },
  "exports": {
    "./form": {
      "types": "./dist/form/index.d.ts",
      "default": "./dist/form/index.js"
    },
    "./views": {
      "types": "./dist/views/index.d.ts",
      "default": "./dist/views/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./layouts": {
      "types": "./dist/layouts/index.d.ts",
      "default": "./dist/layouts/index.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "default": "./dist/icons/index.js"
    },
    "./illustrations": {
      "types": "./dist/illustrations/index.d.ts",
      "default": "./dist/illustrations/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "default": "./dist/theme/index.js"
    },
    "./server-theme": {
      "types": "./dist/theme/server.d.ts",
      "default": "./dist/theme/server.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "bun run rm -rf dist *.tsbuildinfo .turbo node_modules",
    "dev": "bun run tsc -w -p tsconfig.build.json",
    "typecheck": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check . --write"
  },
  "devDependencies": {
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-map-gl": "^6.1.7",
    "@ye/tsconfig": "workspace:^"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "@mui/material": "^6.4.0"
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "@tiptap/core": "^2.11.2",
    "@tiptap/extension-code-block": "^2.11.2",
    "@tiptap/extension-code-block-lowlight": "^2.11.2",
    "@tiptap/extension-image": "^2.11.2",
    "@tiptap/extension-link": "^2.11.2",
    "@tiptap/extension-placeholder": "^2.11.2",
    "@tiptap/extension-text-align": "^2.11.2",
    "@tiptap/extension-underline": "^2.11.2",
    "@tiptap/pm": "^2.11.2",
    "@tiptap/react": "^2.11.2",
    "@tiptap/starter-kit": "^2.11.2",
    "@ye/core": "workspace:^",
    "@ye/env": "workspace:^",
    "@ye/i18n": "workspace:^",
    "@ye/utils": "workspace:^",
    "apexcharts": "^4.3.0",
    "autosuggest-highlight": "^3.3.4",
    "embla-carousel-auto-scroll": "^8.5.1",
    "embla-carousel-react": "^8.5.2",
    "es-toolkit": "^1.31.0",
    "framer-motion": "^11.15.0",
    "mui-one-time-password-input": "^3.0.2",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-apexcharts": "^1.7.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-map-gl": "^7.1.8",
    "react-phone-number-input": "^3.4.11",
    "simplebar-react": "^3.3.0",
    "sonner": "^1.7.2",
    "stylis-plugin-rtl": "^2.1.1"
  }
}

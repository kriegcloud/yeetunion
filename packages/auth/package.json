{
  "name": "@ye/auth",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./src/errors.ts"
    },
    "./supported-oauth": {
      "types": "./dist/supported-oauth.d.ts",
      "default": "./src/supported-oauth.ts"
    },
    "./provider": {
      "types": "./dist/provider/index.d.ts",
      "default": "./src/provider/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "bun run rm -rf dist *.tsbuildinfo .turbo node_modules",
    "dev": "tsc -w -p tsconfig.build.json",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@ye/env": "workspace:^",
    "@ye/utils": "workspace:^",
    "@effect/platform": "^0.68.2",
    "@effect/schema": "latest",
    "effect": "^3.9.1",
    "arctic": "^2.0.1"
  },
  "devDependencies": {
    "@ye/tsconfig": "workspace:^"
  }
}

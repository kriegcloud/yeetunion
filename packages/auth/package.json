{
  "name": "@ye/auth",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./shared": "./src/shared.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "bun run rm -rf dist *.tsbuildinfo .turbo node_modules",
    "dev": "tsc -w -p tsconfig.build.json",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc",
    "generate": "bunx @better-auth/cli@latest generate --output '../db/src/schema/auth-schema.ts' --y"
  },
  "dependencies": {
    "@effect/schema": "latest",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@ye/db": "workspace:^",
    "better-auth": "latest",
    "effect": "latest",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ye/tsconfig": "workspace:^"
  }
}

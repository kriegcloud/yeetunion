{
  "name": "@dank/utils",
  "version": "0.1.0",
  "private": true,
  "description": "Common utilities for TheBois",
  "license": "MIT",
  "author": "The Bois",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./filesystem": {
      "types": "./dist/filesystem.d.ts",
      "default": "./dist/filesystem.js"
    },
    "./url": {
      "types": "./dist/url.d.ts",
      "default": "./dist/url.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "bun run rm -rf dist *.tsbuildinfo .turbo",
    "dev": "tsc -w -p tsconfig.build.json",
    "typecheck": "tsc",
    "lint": "biome check . --write"
  },
  "devDependencies": {
    "@dank/tsconfig": "workspace:^"
  }
}
